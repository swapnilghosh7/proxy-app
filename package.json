{
  "name": "proxy-app",
  "version": "1.0.0",
  "description": "Proxy application with custom headers",
  "main": "electron/main.js",  
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run react-build && electron .",
    "dev": "concurrently \"cross-env BROWSER=none NODE_ENV=development npm run react-start\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "react-start": "webpack serve --mode development",
    "react-build": "webpack --mode production",
    "build": "npm run react-build && electron-builder build --win --mac",
    "build:win": "npm run react-build && electron-builder build --win",
    "build:mac": "npm run react-build && electron-builder build --mac"
  },
  "build": {
    "appId": "com.proxy.app",
    "productName": "Custom Header Proxy",
    "files": [
      "build/**/*",
      "electron/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    }
  },
  "keywords": [
    "proxy",
    "custom-headers",
    "electron",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "find-process": "^1.4.10",
    "http-proxy-middleware": "^3.0.5",
    "ip": "^2.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "electron-is-dev": "^3.0.1",
    "tcp-port-used": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "wait-on": "^8.0.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
